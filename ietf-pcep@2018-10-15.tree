  capability
      +---- gmpls?                 boolean {gmpls}?
      +---- bi-dir?                boolean
      +---- diverse?               boolean
      +---- load-balance?          boolean
      +---- synchronize?           boolean {svec}?
      +---- objective-function?    boolean {objective-function}?
      +---- add-path-constraint?   boolean
