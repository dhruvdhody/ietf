          |  +--rw p2mp?                  boolean {p2mp}?
          |  +--rw stateful {stateful}?
          |  |  +--rw enabled?                boolean
          |  |  +--rw active?                 boolean
          |  |  +--rw pce-initiated?          boolean {pce-initiated}?
          |  |  +--rw include-db-ver?         boolean
          |  |  |       {stateful-sync-opt}?
