          |  |  +--rw incremental-sync?       boolean
          |  |          {stateful-sync-opt}?
          |  +--rw sr {sr}?
          |     +--rw enabled?     boolean
          |     +--rw msd?         uint8
          |     +--rw msd-limit?   boolean
          |     +--rw nai?         boolean
          +--rw pce-info
          |  +--rw scope
