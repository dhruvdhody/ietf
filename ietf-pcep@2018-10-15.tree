      +---- max-dead-timer?         uint32
      +---- min-keep-alive-timer?   uint32
      +---- min-dead-timer?         uint32
      +---- sync-timer?             uint32 {svec}?
      +---- request-timer?          uint32
      +---- max-sessions?           uint32
      +---- max-unknown-reqs?       uint32
      +---- max-unknown-msgs?       uint32

  path-key
      +---- enabled?         boolean
      +---- discard-timer?   uint32
      +---- reuse-time?      uint32
